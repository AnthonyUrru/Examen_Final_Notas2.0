<div class="text-center">    <h1 class="text-body">Notas de proyecto</h1></div><div class="custom-control">    <form class="float-right" action="/" enctype="multipart/form-data" method="get">        <div class="form-group mb-2">            <input class="input-group-text float-lg-left"  placeholder="Ingresa palabra a buscar" id="Nombre" style="background-size:cover; " type="search" name="search">            <button class="btn btn-primary" style="isolation" id="buscar" type="submit"> Buscar</button>        </div>    </form>    <a href="#" data-toggle="modal" data-target="#crear" class="btn btn-primary float-lg-right" style="margin-left:20px">Crear una nota</a></div><br /><br /><div id="nav" class="float-left text-center">     <div class="titulonav">        <h2 class="alert-dark text-body">Tags</h2>    </div>    <div class="category_list">        <div class="container-fluid bg-dark">            <ul class="list-unstyled">                @foreach (var item in ViewBag.Etiquetas)                {            <li  ><a class="container-fluid text-warning" href="#">@item.Name</a></li>                }            </ul>        </div>    </div></div><div class="row" id="Lista"></div><!-- Modal --><div class="modal fade" id="crear" tabindex="-1" aria-labelledby="crear" aria-hidden="true">    <div class="modal-dialog">        <div class="modal-content">            <div class="modal-header text-lg-center bg-info">                <h5 class="modal-title text-body text-lg-center" id="exampleModalLabel" c >Nueva nota</h5>                <button type="button" class="close" data-dismiss="modal" aria-label="Close">                    <span aria-hidden="true">&times;</span>                </button>            </div>            <div id="Cnota" class="modal-body" >            </div>            <div class="modal-footer">                <button type="button" class="btn btn-success" data-dismiss="modal">Cancelar</button>                <button type="button" class="btn btn-warning" onclick="submitFormCrear()">Guardar</button>            </div>        </div>    </div></div>@section Scripts{    <script>        $.ajax({            url: '/Notas/Create',            type: 'get'        }).done(function (html) {            $('#Cnota').html(html);        })        function submitFormCrear() {            $('#createNota').submit();        }        function guardar(event) {            event.preventDefault();            var dataString = new FormData($('#crear form')[0]);            $.ajax({                url: "/Notas/Create",                type: "post",                processData: false,                  contentType: false,                cache: false,                data: dataString            }).done(function (html) {                location.reload();            }).fail(function (html) {                $('#Cnota').html(html.responseText);            });        }    </script>    <script>        function init() {            $.ajax({                url: '/Notas/_Index?search=' + $('#Nombre').val()            }).done(function (notas) {                $('#Lista').html(notas);            });        }        $('#buscar').click(function (e) {            e.preventDefault();            init();        });        init();    </script>}