@{ var nota = (Nota)Model;}<div class="card">    <h3 class="card-header text-center text-warning bg-dark">@nota.Tittle</h3>    <div class="card-body">        <span>            <strong class="text-body">Contenido: </strong>            <span>                @nota.Nota_Content            </span>        </span>        <br />        <br />        <span><strong>Etiquetas: </strong></span>        <ul>            @foreach (var etiqueta in ViewBag.Etiquetas)            {                if (etiqueta.NotaId == nota.Id)                {    <li>        <span>            @etiqueta.Etiqueta.Name        </span>    </li>}}        </ul>        <br />        <span class="font-"><strong> Último Cambio: </strong>@nota.DateRegister</span>    </div></div>